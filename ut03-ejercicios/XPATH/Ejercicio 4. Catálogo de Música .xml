  <!-- Pregunta 1 -->
  <query>//album/title</query>

  <!-- Pregunta 2 -->
  <query>//album[genre='Rock']/title</query>

  <!-- Pregunta 3 -->
  <query>//album[title='21']/price</query>

  <!-- Pregunta 4 -->
  <query>count(//album[stock > 20])</query>

  <!-- Pregunta 5 -->
  <query>//album[price > 18]/artist</query>

  <!-- Pregunta 6 -->
  <query>//album[price = max(//album/price)]</query>

  <!-- Pregunta 7 -->
  <query>//album[title='Thriller']/genre</query>

  <!-- Pregunta 8 -->
  <query>//album[artist='Adele']/@id</query>

  <!-- Pregunta 9 -->
  <query>//album[stock < 30]</query>

  <!-- Pregunta 10 -->
  <query>distinct-values(//album/genre)</query>